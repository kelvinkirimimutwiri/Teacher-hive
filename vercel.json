
{"version": 2,
"builds": [
  {
    "src": "django_web_app/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9.9" }
  },
  {
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles_build"
    }
  }
],
"routes": [
  {
    "src": "/static/(.*)",
    "dest": "/static/$1"
  },
  {
    "src": "/(.*)",
    "dest": "django_web_app/wsgi.py"
  }
]
}